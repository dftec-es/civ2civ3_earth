airlift_max_range="unlimited"
attack_blocked_by="Explode Nuclear","Nuke City","Nuke Units"  # Forbid "Attack" if any one of the listed actions are legal.
suicide_attack_blocked_by="Explode Nuclear","Nuke City","Nuke Units"  # Forbid "Suicide Attack" if any one of the listed actions are legal.
conquer_city_blocked_by="Explode Nuclear","Nuke City","Nuke Units","Attack","Suicide Attack"  # Forbid "Conquer City" if any one of the listed actions are legal.
ui_name_conquer_city_2="%sConquer City from non native%s"

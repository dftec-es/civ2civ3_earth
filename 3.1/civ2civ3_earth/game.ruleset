[datafile]
description="Civ2Civ3 Earth ruleset game data for Freeciv"
ruledit="3.1.0-beta2+"
options="+Freeciv-3.1-ruleset"
format_version=20

[ruledit]
description_file="civ2civ3_earth/README.ruleset_civ2civ3_earth"

[tileset]
preferred="amplio_earth"

[about]
name="Civ2Civ3 Earth ruleset"
version="3.1_2023_06"
summary=_("You are playing a custom version of civ2civ3 rules, from the same author:\n\nIt includes new features, tries to improve balance, and is more suitable for playing on Earth maps, or similar scenarios.\n\nThe differences from the former default ruleset, 'classic', are listed in README.ruleset_civ2civ3_earth.")
description=*civ2civ3_earth/README.ruleset_civ2civ3_earth*
capabilities="std-terrains"

[options]
global_init_buildings="Palace"

[civstyle]
base_pollution=-30
tech_steal_allow_holes=FALSE
tech_trade_allow_holes=FALSE
tech_trade_loss_allow_holes=FALSE
tech_parasite_allow_holes=FALSE
tech_loss_allow_holes=FALSE
upgrade_veteran_loss=1
autoupgrade_veteran_loss=1
granary_food_ini=20,20,20,20,20,30,30,40
granary_food_inc=0
gold_upkeep_style="Mixed"

[illness]
illness_on=TRUE
illness_base_factor=10
illness_min_size=5

[auto_attack]
attack_actions="Capture Units","Bombard","Attack","Suicide Attack"
if_attacker={"type","name","range","present","quiet"
"DiplRel","War","Local",TRUE,TRUE
"MinMoveFrags","1","Local",TRUE,TRUE
"UnitFlag","Nuclear","Local",FALSE,TRUE
"UnitFlag","NoAutoAtt","Local",FALSE,TRUE
}

[actions]
diplchance_initial_odds="Sabotage City","Sabotage City Escape","Targeted Sabotage City Escape","Sabotage City Production Escape","Steal Tech","Steal Tech Escape Expected","Targeted Steal Tech Escape Expected","Incite City","Incite City Escape","Steal Maps","Steal Maps Escape"
bribe_unit_post_success_forced_actions="Transport Embark","Transport Disembark","Transport Disembark 2","Conquer Extras","Conquer Extras 2","Enter Hut","Enter Hut 2","Frighten Hut","Frighten Hut 2","Unit Move"
attack_post_success_forced_actions="Conquer City","Conquer City 2","Transport Disembark","Transport Disembark 2","Conquer Extras","Conquer Extras 2","Enter Hut","Enter Hut 2","Frighten Hut","Frighten Hut 2","Unit Move"
escape_city="Transport Embark","Enter Hut","Enter Hut 2","Frighten Hut","Frighten Hut 2","Unit Move"
unit_stack_death="Transport Embark","Conquer Extras","Conquer Extras 2","Enter Hut","Enter Hut 2","Frighten Hut","Frighten Hut 2","Unit Move"
ui_name_poison_city_escape="%sPoison City%s"
ui_name_sabotage_city_escape="%sSabotage City Escape%s"
ui_name_targeted_sabotage_city_escape="Industrial %sSabotage%s"
ui_name_targeted_steal_tech_escape="Indus%strial Espionage%s"
ui_name_incite_city="Incite a %sRevolt%s"
ui_name_enter_marketplace="%sCancel Trade Routes (after end of turn)%s"
ui_name_bribe_unit="%sBribe Enemy Unit%s"
ui_name_sabotage_unit_escape="%sSabotage Enemy Unit%s"
ui_name_found_city="%sBuild City%s"
found_city_consuming_always=TRUE
ui_name_join_city="%sAdd to City%s"
ui_name_steal_maps="Steal %sMap Fragments"
ui_name_steal_maps_escape="Steal %sMap Fragments and Escape%s"
explode_nuclear_target_kind="Tile"
explode_nuclear_consuming_always=TRUE
nuke_city_target_kind="City"
nuke_city_min_range=1
nuke_city_max_range=1
nuke_city_consuming_always=TRUE
